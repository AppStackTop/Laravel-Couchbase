{
  "name": "ytake/laravel-couchbase",
  "description": "Couchbase providers for Laravel",
  "keywords": [
    "laravel",
    "couchbase",
    "database",
    "session",
    "cache"
  ],
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.1",
    "illuminate/config": "~5.1",
    "illuminate/console": "~5.1",
    "illuminate/events": "~5.1",
    "illuminate/cache": "~5.1",
    "illuminate/session": "~5.1",
    "illuminate/database": "~5.1",
    "illuminate/encryption": "~5.1"
  },
  "require-dev": {
    "symfony/framework-bundle": "~2.0|~3.0",
    "symfony/console": "~2.0|~3.0",
    "phpunit/phpunit": "4.*",
    "satooshi/php-coveralls": "*",
    "phing/phing": "~2.0",
    "fabpot/php-cs-fixer": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\LaravelCouchbase\\": "src"
    },
    "files": [
      "src/transfer.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/CouchbaseTestCase.php"
    ]
  },
  "suggest": {
    "ext-couchbase": "for n1ql query builder, use bucket type couchbase require"
  },
  "scripts": {
    "test": [
      "php vendor/bin/phpunit"
    ],
    "cs": [
      "php vendor/fabpot/php-cs-fixer/php-cs-fixer fix"
    ]
  },
  "minimum-stability": "stable"
}

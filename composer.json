{
  "name": "ytake/laravel-couchbase",
  "description": "Couchbase providers for Laravel4(supported only memcached bucket)",
  "keywords": [
    "laravel",
    "couchbase",
    "session",
    "cache"
  ],
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~4.0",
    "illuminate/config": "~4.0",
    "illuminate/console": "~4.0",
    "illuminate/events": "~4.0",
    "illuminate/cache": "~4.0",
    "illuminate/session": "~4.0",
    "illuminate/encryption": "~4.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "2.*",
    "symfony/console": "2.*",
    "phpunit/phpunit": "4.*",
    "satooshi/php-coveralls": "*",
    "phing/phing": "~2.0",
    "fabpot/php-cs-fixer": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\LaravelCouchbase\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/CouchbaseTestCase.php"
    ]
  },
  "minimum-stability": "stable"
}
